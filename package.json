{
  "name": "expo-react-native-setup-cli",
  "version": "1.0.0",
  "description": "Boilerplate for an expo react-native project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/index.js",
    "build": "rm -rf lib && esbuild src/index.js --format=cjs --platform=node --bundle --outfile=lib/index.cjs --minify --analyze",
    "prepublish": "npm run build",
    "example": "npx nxrn create example",
    "relink": "npm unlink nxrn && npm run prepublish && npm link",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "edsycarreon",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^10.0.2",
    "ora": "^8.0.1",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "esbuild": "^0.23.0"
  }
}
